    <script type="text/javascript">
        /* Setting the click event on body so that the setConsent would get called when the user clicks on anywhere in the page */
        /*document.body.addEventListener('click', setConsent, true);*/

        function changeCookies() {
            var analyticsConsent = siteConsent.getConsentFor(WcpConsent.consentCategories.Analytics);

            if (analyticsConsent) {
                setAnalyticsCookies();
            } else {
                /* This section should remove all Adobe Analytics cookies by setting their expiration to a previous date. */
                var decodedCookie = decodeURIComponent(document.cookie);
                var cookieJar = decodedCookie.split(';');
                for (var i = 0; i < cookieJar.length; i++) {
                    var cookie = cookieJar[i];
                    while (cookie.charAt(0) == ' ') {
                        cookie = cookie.substring(1);
                    }
                    var name = cookie.split('=')[0];
                    if (name.includes("AdobeOrg")) {
                        document.cookie = name + "=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
                    }
                }
            }
        }


        function setAnalyticsCookies() {
            var bingScript = document.getElementById('bingMapScript');
            if (bingScript.src === '') {
                loadBingMapScript();
            }
            var script = document.createElement('script');
            script.src = "https://query.prod.cms.rt.microsoft.com/cms/api/am/binary/RE1r2ij";
            script.type = "text/javascript";
            document.head.appendChild(script);
        }
        WcpConsent.onConsentChanged(changeCookies);

    </script>


