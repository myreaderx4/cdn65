    <script type="text/javascript">
        function loadfooter() {
            document.getElementById("footerx").innerHTML = document.getElementById('myHtml2').innerHTML;
            //Added Code for removing tab and mouse click on language picker
            var x = document.getElementById("locale-picker-link");
